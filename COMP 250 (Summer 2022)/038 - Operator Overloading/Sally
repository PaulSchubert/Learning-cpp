
#include <iostream>
#include "Sally.h"

using namespace std;

Sally :: Sally()
{}

Sally :: Sally(int a){
    num = a;
}

Sally Sally :: operator+(Sally aso){ //aso = another sally object
    Sally brandNew;     //creating new sally object
    brandNew.num = num + aso.num;   //assigning number to new sally object
    return(brandNew);   //returning new sally object
}
