#include <iostream>
#include "Square.h"

using namespace std;

//constructors
Square :: Square(){
    width = 0;
    length = 0;
}
Square::Square(double w, double l){
    width = w;
    length = l;
}
    
//set functions
void Square :: setWidth(double w){
    width = w;
}
void Square :: setLength(double l){
    length = l;
}
    
//get functions
double Square :: getWidth(){
    return width;
}
double Square :: getLength(){
    return length;
}
double Square :: getArea(){
    return length * width;
}

